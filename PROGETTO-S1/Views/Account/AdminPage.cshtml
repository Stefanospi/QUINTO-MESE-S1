@model PROGETTO_S1.Models.AdminPageViewModel

<h1>Spedizioni In Consegna Oggi</h1>

<!-- Tabella per visualizzare le spedizioni in consegna -->


<table class="table">
    <thead>
        <tr>
            <th>Id Spedizione</th>
            <th>Cliente Azienda</th>
            <th>Cliente Privato</th>
            <th>Num Id</th>
            <th>Data Spedizione</th>
            <th>Peso</th>
            <th>Città Destinatario</th>
            <th>Indirizzo</th>
            <th>Nome Destinatario</th>
            <th>Costo Spedizione</th>
            <th>Data Consegna Prevista</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var spedizione in Model.Spedizioni)
        {
            <tr>
                <td>@spedizione.IdSpedizione</td>
                <td>@spedizione.FK_ClienteAzienda</td>
                <td>@spedizione.FK_ClientePrivato</td>
                <td>@spedizione.NumId</td>
                <td>@spedizione.DataSpedizione</td>
                <td>@spedizione.Peso</td>
                <td>@spedizione.CittaDestinatario</td>
                <td>@spedizione.Indirizzo</td>
                <td>@spedizione.NomeDestinatario</td>
                <td>@spedizione.CostoSpedizione</td>
                <td>@spedizione.DataConsegnaPrev</td>
            </tr>
        }
    </tbody>
</table>
<h2>Numero totale di spedizioni non consegnate: @Model.TotSpedizioniNonConsegnate</h2>

<!-- Resto del codice della vista -->
